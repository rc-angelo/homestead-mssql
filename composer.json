{
    "name": "f3rland/homestead-mssql",
    "description": "A virtual machine for web artisans that include drivers for SQL Server.",
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/process": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Remi Ferland",
            "email": "f3rland@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Homestead\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "bin": [
        "bin/homestead"
    ]
}
